{
  "expo": {
    "name": "app",
    "slug": "app",
    "assets": [
      "./App/Assets/eco.ttf"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./App/Assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./App/Assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to scan barcodes on product."
      },
      "bundleIdentifier": "com.neety13.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./App/Assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "blockedPermissions": [
        "android.permission.CAMERA"
      ]
    },
    "web": {
      "favicon": "./App/Assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "828943df-303e-45f9-8f75-d83a900a0911"
      }
    }
  }
}
